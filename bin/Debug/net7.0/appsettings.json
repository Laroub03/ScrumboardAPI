{
  "AppSettings": {
    "Token": "my top secret key"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "HttpsFromPem": {
        "ClientCertificateMode": "RequireCertificate",
        "Url": "https://*:8443",
        "Certificate": {
          "Path": "mkcertPems/localhost+3.pem",
          "KeyPath": "mkcertPems/localhost+3-key.pem"
        }
      }
    }
  }
}
